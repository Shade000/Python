def is_full_house(cards):
    karty = ['A','J','Q','K','2','3','4','5','6','7','8','9','10']
    counter = 0
    for i in karty:
        if cards.count(i) == 2 or cards.count(i) == 3:
            counter += 1
    if counter == 2:
        return True
    else: 
        return False
        
is_full_house(["A", "A", "A", "K", "K"])
